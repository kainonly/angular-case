{
  "name": "think/api_case",
  "description": "Example of thinkphp use",
  "type": "project",
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "topthink/framework": "^6.0.0",
    "topthink/think-orm": "^2.0",
    "topthink/think-multi-app": "^1.0",
    "topthink/think-queue": "^3.0",
    "topthink/think-view": "^1.0",
    "ramsey/uuid": "^3.8",
    "lcobucci/jwt": "^3.3",
    "ircmaxell/random-lib": "^1.2",
    "danielstjules/stringy": "^3.1",
    "doctrine/collections": "^1.6",
    "phpseclib/phpseclib": "^2.0",
    "nesbot/carbon": "^2.10",
    "predis/predis": "^1.1",
    "aliyuncs/oss-sdk-php": "^2.3",
    "elasticsearch/elasticsearch": "^7.2",
    "guzzlehttp/guzzle": "^6.3",
    "kain/simplify-amqp": "^1.0",
    "non0/huaweicloud-obs-sdk": "^3.20"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/var-dumper": "^4.2",
    "topthink/think-trace": "^1.0",
    "topthink/think-migration": "^3.0",
    "phpunit/phpunit": "^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app",
      "think\\bit\\": "library/think-bit/src/",
      "think\\extra\\": "library/think-extra/src/",
      "think\\support\\": "library/think-support/src/",
      "think\\redis\\": "library/think-redis/src/",
      "think\\elastic\\": "library/think-elastic/src/",
      "think\\amqp\\": "library/think-amqp/src/",
      "think\\aliyun\\extra\\": "library/think-aliyun-extra/src/",
      "think\\huaweicloud\\extra\\": "library/think-huaweicloud-extra/src/"
    },
    "files": [
      "library/think-extra/src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "ExtraTests\\": "library/think-extra/tests/",
      "RedisTests\\": "library/think-redis/tests/",
      "AmqpTests\\": "library/think-amqp/tests/",
      "ElasticTests\\": "library/think-elastic/tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php think service:discover",
      "@php think vendor:publish"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
